pipeline {
    agent any
      
       parameters{
         string(name:'Version', defaultValue: '1.x', description: 'which version you want to deploy')
         }
     
        stages {
            stage ('Get the Artifact from Repository') {
                steps{
                    sh " curl -o http://192.168.189.246:8081/repository/maven-public/com/wakaleo/gameoflife/gameoflife-web/$Version/*.war "
                }
            }
            
                }
         
}
